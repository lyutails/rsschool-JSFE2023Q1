{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    // "baseUrl": "./src",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": true,
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    // "paths": {
    //   "components/": ["components/"]
    // }
  },
  "include": ["src"]
}
